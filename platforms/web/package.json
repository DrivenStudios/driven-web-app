{
  "name": "@jwp/ott-web",
  "version": "4.30.0",
  "private": true,
  "author": "JW Player",
  "main": "index.js",
  "engines": {
    "node": ">=18.13.0"
  },
  "scripts": {
    "build": "vite build --mode ${MODE:=prod} && sh scripts/compressIni.sh build/public/.webapp.ini",
    "codecept-serve:desktop": "yarn codecept:desktop ; yarn serve-report:desktop",
    "codecept-serve:mobile": "yarn codecept:mobile ; yarn serve-report:mobile",
    "codecept:desktop": "cd test-e2e && rm -rf \"./output/desktop\" && codeceptjs run-workers --suites ${WORKER_COUNT:=8} --config ./codecept.desktop.cjs",
    "codecept:mobile": "cd test-e2e && rm -rf \"./output/mobile\" && codeceptjs run-workers --suites ${WORKER_COUNT:=8} --config ./codecept.mobile.cjs",
    "generate-pwa-assets": "pwa-assets-generator",
    "i18next": "i18next-workspaces parse ./ && i18next-workspaces namespaces ./ src/i18n/resources.ts",
    "lint:ts": "tsc --pretty --noEmit -p ./ && tsc --pretty --noEmit -p ./test-e2e",
    "serve-report:desktop": "cd test-e2e && allure serve \"./output/desktop\"",
    "serve-report:mobile": "cd test-e2e && allure serve \"./output/mobile\"",
    "start": "vite",
    "start:test": "vite build --mode test && vite preview --port 8080",
    "test": "TZ=UTC LC_ALL=en_US.UTF-8 vitest run",
    "test-coverage": "TZ=UTC LC_ALL=en_US.UTF-8 vitest run --coverage",
    "test-update": "TZ=UTC LC_ALL=en_US.UTF-8 vitest run --update",
    "test-watch": "TZ=UTC LC_ALL=en_US.UTF-8 vitest"
  },
  "dependencies": {
    "@codeceptjs/allure-legacy": "^1.0.2",
    "@jwp/ott-common": "*",
    "@jwp/ott-hooks-react": "*",
    "@jwp/ott-ui-react": "*",
    "broadcast-channel": "^7.0.0",
    "i18next": "^24.0.5",
    "i18next-browser-languagedetector": "^6.1.8",
    "i18next-http-backend": "^2.7.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^15.1.3",
    "react-router": "6.14.2",
    "react-router-dom": "6.14.2",
    "wicg-inert": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@codeceptjs/configure": "^0.8.0",
    "@jwp/ott-testing": "*",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jwplayer": "^8.31.3",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-helmet": "^6.1.11",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^0.33.0",
    "allure-commandline": "^2.32.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "codeceptjs": "^3.6.7",
    "core-js": "^3.39.0",
    "eslint-config-jwp": "*",
    "eslint-plugin-codeceptjs": "^1.3.0",
    "i18next-parser": "^8.13.0",
    "jsdom": "^22.1.0",
    "luxon": "^3.5.0",
    "playwright": "^1.49.0",
    "postcss": "^8.4.49",
    "postcss-config-jwp": "*",
    "react-app-polyfill": "^3.0.0",
    "reflect-metadata": "^0.2.2",
    "sass-embedded": "^1.81.1",
    "stylelint": "^15.11.0",
    "stylelint-config-jwp": "*",
    "tsconfig-paths": "^4.2.0",
    "vi-fetch": "^0.8.0",
    "vite": "^5.4.11",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-pwa": "^0.19.8",
    "vite-plugin-static-copy": "^2.2.0",
    "vite-plugin-stylelint": "^5.3.1",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^2.1.8",
    "vitest-axe": "^1.0.0-pre.3",
    "workbox-build": "^6.6.1",
    "workbox-window": "^6.6.1"
  },
  "optionalDependencies": {
    "gh-pages": "^6.1.1",
    "sharp": "^0.33.4"
  }
}
